{{ ttchotline_rails_env }}:
  adapter: postgresql
  timeout: 5000
  encoding: utf8
  reconnect: false
  database: {{ ttchotline_database_name }}
  pool: 5
  username: {{ ttchotline_database_user }}
  password: {{ ttchotline_database_password }}
  host: {{ ttchotline_database_host }}
  port: {{ ttchotline_database_port }}